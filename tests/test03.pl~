#!/usr/bin/perl -w
# Test for Subset2 - Operators

$string1 = "Hello";
$string2 = "Goodbye";

if ($string2 eq "Hello" || $string2 ge "Goodbye") {
	print $string1, "\n";
} elsif ($string1 ge "Good") {
	print "$string2\n";
} else {
	print "World\n";
}

while ($string1 ne "Hello") {
	print "break\n";
	last;
}
