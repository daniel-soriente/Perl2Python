#!/usr/bin/perl -w
# Demo for Subset1
# Variables, Numerical Constants, Arithmetic Operators - Remains unchanged besides '$' and ';'

$num = 585148190;
$string = "ashfoifnndqwnriqhrianflwnfeahfaljfaqifjpjr	F	DQW]DW[LKOQW";

print "$num\n";
print "$string\n";

$num = $num + $num;
$num = $num - 129581401354;
$num = 185918492 * $num;
$num=$num/1;

print "$num";

